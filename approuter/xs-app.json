{
    "welcomeFile": "index.html",
    "authenticationMethod": "route",
    "logout": {
        "logoutEndpoint": "/do/logout"
    },
    "routes": [{
            "source": "^/(index.html)$",
            "target": "$1",
            "localDir": ".",
            "authenticationType": "none"
        },
        {
            "source": "^/jmodule1(.*)$",
            "target": "$1",
            "destination": "jmodule",
            "scope": "$XSAPPNAME.Read",
            "authenticationType": "xsuaa"
        },
        {
            "source": "^/pymodule1(.*)$",
            "target": "$1",
            "destination": "pymodule",
            "authenticationType": "none"
        }
    ]
}